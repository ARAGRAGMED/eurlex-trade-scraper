# EUR-Lex Web Scraping Credentials
# These are used for accessing EUR-Lex advanced search (not SOAP API)
EURLEX_USERNAME=your_eurlex_username
EURLEX_PASSWORD=your_eurlex_password

# Note: SOAP API is no longer used - we switched to web scraping approach
# EURLEX_WSDL_URL is kept for reference but not actively used
EURLEX_WSDL_URL=https://eur-lex.europa.eu/EURLexWebService?wsdl

# Optional: Application settings
LOG_LEVEL=INFO
DATA_DIR=./data

# Optional: For Vercel deployment
VERCEL_ENV=production
